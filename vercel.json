{
  "redirects": [
    {
      "source": "/",
      "destination": "/en",
      "permanent": true
    },
    {
      "source": "/en/:path*",
      "destination": "/en/:path*",
      "permanent": true
    },
    {
      "source": "/tr/:path*",
      "destination": "/tr/:path*",
      "permanent": true
    },
    {
      "source": "/:path((?!tr|en).*)",
      "destination": "/en",
      "permanent": true
    }
  ]
}
